#include <stdio.h>

int main() {
    float percentage;

    // Taking input from user
    printf("Enter the student's percentage: ");
    scanf("%f", &percentage);

    // Checking grade conditions
    if (percentage >= 95) {
        printf("Grade A");
    }
    else if (percentage >= 85) {
        printf("Grade B");
    }
    else if (percentage >= 75) {
        printf("Grade C");
    }
    else if (percentage >= 65) {
        printf("Grade D");
    }
    else if (percentage >= 45) {
        printf("Grade E");
    }
    else {
        printf("Grade F");
    }

    return 0;
}
