#include <stdio.h>

int main() {
    int angle;

    printf("Enter angle (0, 90, 180, 270): ");
    scanf("%d", &angle);

    if (angle == 0) {
        printf("Direction: East");
    }
    else if (angle == 90) {
        printf("Direction: North");
    }
    else if (angle == 180) {
        printf("Direction: West");
    }
    else if (angle == 270) {
        printf("Direction: South");
    }
    else {
        printf("Invalid angle! Please enter 0, 90, 180 or 270.");
    }

    return 0;
}
